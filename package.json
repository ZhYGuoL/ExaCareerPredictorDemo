{
  "name": "career-paths",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "typecheck": "tsc --noEmit",
    "migrate": "wrangler d1 migrations apply app --local",
    "sql": "wrangler d1 execute app --command",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier -w .",
    "check": "npm run typecheck && npm run lint",
    "loadtest": "ts-node scripts/loadtest.ts http://localhost:8787/rank/final 100 10"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240925.0",
    "@eslint/js": "^9.37.0",
    "@types/node": "^24.6.2",
    "@types/node-fetch": "^2.6.11",
    "eslint": "^9.36.0",
    "node-fetch": "^2.7.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.45.0",
    "wrangler": "^3.78.7"
  }
}
